# Backend Environment Variables Example

# Node Environment
NODE_ENV=production

# API Port
API_PORT=3000

# Database
DATABASE_PATH=/app/data/database.sqlite

# Download Settings
MAX_DOWNLOAD_CONCURRENT=3
DOWNLOAD_SPEED_LIMIT=0

# Logging
LOG_LEVEL=info

# CORS
CORS_ORIGIN=*

# Session
SESSION_SECRET=change-this-to-a-random-secret-key
